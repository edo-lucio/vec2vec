#!/bin/bash

#SBATCH --job-name=simple        # Job name
#SBATCH --output=job.%j.out      # Name of output file (%j expands to jobId)
#SBATCH --cpus-per-task=1        # Schedule one core
#SBATCH --time=01:00:00          # Run time (hh:mm:ss) - run for one hour max
#SBATCH --gres=gpu         # Run time (hh:mm:ss) - run for one hour max
#SBATCH --partition=scavenge    # Run on scavenge queue (all nodes, low priority)

apptainer exec --nv -B /home/edlu/vec2vec/:/v2v vec.sif bash -c "
    micromamba run -n vec python -s /v2v/eval.py finetuning_unsupervised/e:1000,n:1000000,s:clap,u:e5 unsupervised
"